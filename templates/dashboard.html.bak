{% include "includes/header.html" with title="Tableau des dépôts" %}

<div class="h-full w-full flex ">

    {% include "includes/menu.html" with title="Tableau des dépôts" %}

    <div class="w-full h-full p-1 relative" style="background: #fafbfd;">

        <div class="loading-div z-10 absolute left-0 top-0 bottom-0 right-0" style="background: #fafbfd;">
            <div class="absolute inset-center text-sm flex">
                <svg class="animate-spin -ml-1 mr-3 h-5 w-5 text-gray-600" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24">
                  <circle class="opacity-25" cx="12" cy="12" r="10" stroke="currentColor" stroke-width="4"></circle>
                  <path class="opacity-75" fill="currentColor" d="M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z"></path>
                </svg>
                Chargement des données
            </div>
        </div>

        <!-- Si version dev changer   src="/kibana/app/dashboards#  en   src="http://127.0.0.1:5601/app/dashboards#  -->
        {% if type == "rsr" %}
        <div class="w-full h-full">
           <iframe src="/kibana/app/dashboards#/view/2e1ef800-c1f2-11eb-b466-1b2443e30920?embed=true&_g=(filters:!((meta:(alias:'{{ key }}',key:{{ ext_key }},params:(query:{{ key }}),type:phrase),query:(match_phrase:({{ ext_key }}:{{ key }}))),(meta:(alias:'validés',key:validated,params:(query:!t),type:phrase),query:(match_phrase:(validated:!t)))),time:({{ timeRange }}))" height="100%" width="100%"></iframe>

        </div>
        {% else %}
 {% comment %}           <div class="w-full h-full">
            <iframe src="/kibana/app/dashboards#/view/3426d960-c1fd-11eb-9da8-399e11029bf2?embed=true&_g=(filters:!(),refreshInterval:(pause:!t,value:0),time:({{ timeRange }}))&hide-filter-bar=true&_a=(query:(query_string:(analyze_wildcard:!t,query:'{{ filterlab  }}')))" height="100%" width="100%"></iframe>
        </div>{% endcomment %}

            <div class="w-full h-full overflow-auto">
                <iframe src="/kibana/app/dashboards#/view/99379510-a113-11ec-91a3-27eaa11a874e?embed=true&_g=(filters:!(),refreshInterval:(pause:!t,value:0),time:({{ timeRange }}))&show-query-input=true&show-time-filter=true&_a=(query:(query_string:(analyze_wildcard:!t,query:'{{ filterlabB  }}')))" height="100%" width="100%"></iframe>
                <iframe src="/kibana/app/dashboards#/view/4a37d8e0-a112-11ec-91a3-27eaa11a874e?embed=true&_g=(filters:!(),refreshInterval:(pause:!t,value:0),time:(from:now-25y,to:now))&show-query-input=true&show-time-filter=true&_a=(query:(query_string:(analyze_wildcard:!t,query:'{{ filterlabA  }}')))" height="100%" width="100%"></iframe>
                <iframe src="/kibana/app/dashboards#/view/13661560-a122-11ec-91a3-27eaa11a874e?embed=true&_g=(filters:!(),refreshInterval:(pause:!t,value:0),time:({{ timeRange }}))&show-query-input=true&show-time-filter=true&_a=(query:(query_string:(analyze_wildcard:!t,query:'{{ filterlabA  }}')))" height="100%" width="100%"></iframe>
    </div>

        {% endif %}

    </div>
</div>
{% include "includes/footer.html" %}
